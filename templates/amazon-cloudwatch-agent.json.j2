{
  "agent": {
    "region": "{{ cw_region }}",
    "run_as_user": "root"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "{{ docker_log_path }}",
            "log_group_name": "{{ log_group_docker }}",
            "log_stream_name": "{instance_id}",
            "timezone": "UTC"
          }
        ]
      }
    }
  }
}
